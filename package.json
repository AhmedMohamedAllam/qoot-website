{
  "name": "qoot-website",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config apps/marketing/vite.config.js",
    "dev:marketing": "vite --config apps/marketing/vite.config.js",
    "dev:customer": "vite --config apps/customer/vite.config.js",
    "dev:dashboard": "vite --config apps/dashboard/vite.config.js",
    "dev:all": "concurrently \"npm run dev:marketing\" \"npm run dev:customer\" \"npm run dev:dashboard\"",
    "build": "npm run build:marketing && npm run build:customer && npm run build:dashboard",
    "build:marketing": "vite build --config apps/marketing/vite.config.js",
    "build:customer": "vite build --config apps/customer/vite.config.js",
    "build:dashboard": "vite build --config apps/dashboard/vite.config.js",
    "lint": "eslint .",
    "preview": "vite preview --config apps/marketing/vite.config.js",
    "preview:marketing": "vite preview --config apps/marketing/vite.config.js",
    "preview:customer": "vite preview --config apps/customer/vite.config.js",
    "preview:dashboard": "vite preview --config apps/dashboard/vite.config.js"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "date-fns": "^3.6.0",
    "firebase": "^12.7.0",
    "framer-motion": "^12.23.26",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.1.1",
    "recharts": "^2.15.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tailwindcss": "^4.1.18",
    "vite": "^7.2.4"
  }
}
